package com.multigold.mdm.dto.common;

import java.io.Serializable;

/**
 * 
 * @author yuefan
 *
 */
public class GenericResponseHeader implements Serializable {

	private static final long serialVersionUID = -50654568185794250L;
	
	public GenericResponseHeader() {
		
	}

	public GenericResponseHeader(String responseCode, String remark) {
		super();
		this.responseCode = responseCode;
		this.remark = remark;
	}

	private String responseCode = "";
	
	private String remark = "";
	
	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getResponseCode() {
		return responseCode;
	}

	public void setResponseCode(String responseCode) {
		this.responseCode = responseCode;
	}
	
}
