function determine(flag,ids,sums,coCodes){
	sums = sums.toFixed(4);
	$.messager.show({
		"title" : "操作成功"
	});
	if(flag == "inv1"){
		$('#storageId1').val(ids);
		$('#storageIdMoneyTotal1').val(sums);
		$('#coCode1').val(coCodes);
	}else if(flag == "inv2"){
		$('#storageId2').val(ids);
		$('#storageIdMoneyTotal2').val(sums);
		$('#coCode2').val(coCodes);
	}
	$('#data_detail_dialog').dialog('close');
}

function checkOther(obj){
	var str = obj.value;
	var reg =/\d{4}-\d{2}-\d{2}/; 
	if(str!=null&&str!="")
   	{
		if (!reg.test(str)){
			alert("日期格式不正确！应为YYYY-MM-DD");
			obj.focus();
		}
   	}
}

